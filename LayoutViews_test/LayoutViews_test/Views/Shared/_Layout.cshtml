<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - LayoutViews_test</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/LayoutViews_test.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <ul>
            <li><a asp-route="Home">Home</a></li>
            <li><a asp-route="about-company">about-company</a></li>
            <li><a asp-route="contact-support">contact-support</a></li>
            <li><a asp-route="products">products</a></li>
            <li><a asp-route="search-products">search-products</a></li>
            <li><a asp-route="order-product">order-product</a></li>
        </ul>
   </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - LayoutViews_test - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
